CREATE TABLE TB_ENDERECO (
ID_ENDERECO int auto_increment PRIMARY KEY,
RUA_ENDERECO VARCHAR (60),
NUMERO_ENDERECO INT,
BAIRRO_ENDERECO VARCHAR(60),
CIDADE_ENDERECO VARCHAR(60),
ESTADO_ENDERECO VARCHAR(25),
CEP_ENDERECO INT,
ID_USUARIO int,
CONSTRAINT fk_enduser FOREIGN KEY (ID_USUARIO) REFERENCES TB_USUARIO (ID_USUARIO)
);